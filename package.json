{
  "name": "metalsmith-highline",
  "version": "0.0.0",
  "engines": {
    "node": "0.10.x"
  },
  "description": "A Highline plugin for Metalsmith.",
  "keywords": [
    "metalsmith",
    "markdown",
    "github",
    "code"
  ],
  "main": "./lib/",
  "scripts": {
    "style": "./node_modules/.bin/crispy ./lib/ ./test/",
    "test": "npm run style && ./node_modules/.bin/mocha --reporter spec -r should --grep ''"
  },
  "dependencies": {
    "bluebird": "^2.9.13",
    "highline": "^1.0.0",
    "joi": "^6.0.5",
    "multimatch": "^2.0.0"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "crispy": "^3.1.2",
    "metalsmith": "^1.3.0",
    "metalsmith-markdown": "^0.2.1",
    "mocha": "^2.2.1",
    "should": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-highline"
  }
}
